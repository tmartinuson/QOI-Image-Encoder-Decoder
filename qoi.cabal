name: qoi
version: 0.1.0.0
build-type: Simple
cabal-version: >=1.10

executable qoi
  main-is: qoi.hs
  ghc-options:
    -O3
    -threaded
  default-language: Haskell2010
  build-depends:
    base >= 4.7 && < 5,
    async,
    hip,
    vector,
    containers,
    binary,
    bytestring,
    directory,
    filepath